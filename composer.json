{
    "name": "aedart/athenaeum-etags",
    "description": "ETags utilities for your Laravel Application",
    "keywords": ["Athenaeum", "ETag", "Entity Tag", "Generator", "HTTP conditional requests"],
    "homepage": "https://aedart.github.io/athenaeum/",
    "license": "BSD-3-Clause",
    "type": "library",
    "authors": [
        {
            "name": "Alin Eugen Deac",
            "email": "aedart@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "aedart/athenaeum-contracts": "^6.9",
        "aedart/athenaeum-support": "^6.9",
        "aedart/athenaeum-utils": "^6.9",
        "illuminate/http": "^v9.30.1",
        "ramsey/http-range": "^1.1.0"
    },
    "autoload": {
        "psr-4": {
            "Aedart\\ETags\\": "src"
        }
    },
    "suggest": {
        "ramsey/http-range": "If you want to evaluate If-Range/Range header fields and validate requested range(s)"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
